@model Company.Models.Организации

<form asp-action="Create">
    <div class="form-group">
        <label asp-for="Название_организации" class="control-label"></label>
        <input asp-for="Название_организации" class="form-control" />
        <span asp-validation-for="Название_организации" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="ИНН" class="control-label"></label>
        <input asp-for="ИНН" class="form-control" />
        <span asp-validation-for="ИНН" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="КПП" class="control-label"></label>
        <input asp-for="КПП" class="form-control" />
        <span asp-validation-for="КПП" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="ОГРН" class="control-label"></label>
        <input asp-for="ОГРН" class="form-control" />
        <span asp-validation-for="ОГРН" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Дата_закрытия" class="control-label"></label>
        <input asp-for="Дата_закрытия" class="form-control" />
        <span asp-validation-for="Дата_закрытия" class="text-danger"></span>
    </div>
    <div class="form-group">
        <input type="submit" value="Create" class="btn btn-primary" />
    </div>
</form>
